			TweenService:Create(line, TweenInfo.new(0.2), {BackgroundColor3 = COLOR_NEON_PINK}):Play()
		end
	end
end)

-- Tornar o bot√£o Night arrast√°vel pelo √≠cone
local nightDragging = false
local nightDragStart = nil
local nightStartPos = nil

dragIcon.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		nightDragging = true
		nightDragStart = input.Position
		nightStartPos = nightBtn.Position
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if nightDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
		local delta = input.Position - nightDragStart
		nightBtn.Position = UDim2.new(nightStartPos.X.Scale, nightStartPos.X.Offset + delta.X, nightStartPos.Y.Scale, nightStartPos.Y.Offset + delta.Y)
	end
end)

UserInputService.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		nightDragging = false
	end
end)

nightClickBtn.MouseButton1Click:Connect(function()
	if not nightDragging then
		toggleMinimize()
	end
end)

-- Fun√ß√£o para minimizar/maximizar
local menuMinimized = false
function toggleMinimize()
	menuMinimized = not menuMinimized
	if menuMinimized then
		-- Minimizar: esconder menu e mostrar bot√£o Night
		mainFrame.Visible = false
		nightBtn.Visible = true
		panelVisible = false
		-- Restaurar mouse
		UserInputService.MouseBehavior = Enum.MouseBehavior.Default
		UserInputService.MouseIconEnabled = true
	else
		-- Maximizar: mostrar menu e esconder bot√£o Night
		mainFrame.Visible = true
		nightBtn.Visible = false
		panelVisible = true
		-- Liberar cursor
		UserInputService.MouseBehavior = Enum.MouseBehavior.Default
		UserInputService.MouseIconEnabled = true
	end
end

minimizeBtn.MouseButton1Click:Connect(function()
	toggleMinimize()
end)

-- Tabs
tabs = {}
currentTab = "PLAYERS"

function createTab(name, text, icon, order)
	local tab = Instance.new("TextButton")
	tab.Name = name .. "Tab"
	tab.Size = UDim2.new(0, 70, 0, 70)
	tab.BackgroundColor3 = COLOR_BG_PANEL
	tab.BackgroundTransparency = 0.5
	tab.Text = ""
	tab.BorderSizePixel = 0
	tab.LayoutOrder = order
	tab.Parent = sidebar

	local tabCorner = Instance.new("UICorner")
	tabCorner.CornerRadius = UDim.new(0, 14)
	tabCorner.Parent = tab

	local tabStroke = Instance.new("UIStroke")
	tabStroke.Color = COLOR_NEON_PINK
	tabStroke.Thickness = 0
	tabStroke.Transparency = 0.5
	tabStroke.Parent = tab

	local iconLabel = Instance.new("TextLabel")
	iconLabel.Name = "Icon"
	iconLabel.Size = UDim2.new(1, 0, 0, 35)
	iconLabel.Position = UDim2.new(0, 0, 0, 8)
	iconLabel.BackgroundTransparency = 1
	iconLabel.Text = icon
	iconLabel.Font = Enum.Font.GothamBold
	iconLabel.TextSize = 28
	iconLabel.TextColor3 = COLOR_TEXT_INACTIVE
	iconLabel.Parent = tab

	local nameLabel = Instance.new("TextLabel")
	nameLabel.Name = "Name"
	nameLabel.Size = UDim2.new(1, 0, 0, 20)
	nameLabel.Position = UDim2.new(0, 0, 1, -25)
	nameLabel.BackgroundTransparency = 1
	nameLabel.Text = text
	nameLabel.Font = Enum.Font.Gotham
	nameLabel.TextSize = 10
	nameLabel.TextColor3 = COLOR_TEXT_INACTIVE
	nameLabel.Parent = tab

	tabs[name] = tab

	tab.MouseEnter:Connect(function()
		if currentTab ~= name then
			TweenService:Create(tab, TweenInfo.new(0.2), {BackgroundColor3 = COLOR_BG_HOVER}):Play()
			TweenService:Create(iconLabel, TweenInfo.new(0.2), {TextColor3 = COLOR_NEON_PINK}):Play()
		end
	end)
	tab.MouseLeave:Connect(function()
		if currentTab ~= name then
			TweenService:Create(tab, TweenInfo.new(0.2), {BackgroundColor3 = COLOR_BG_PANEL, BackgroundTransparency = 0.5}):Play()
			TweenService:Create(iconLabel, TweenInfo.new(0.2), {TextColor3 = COLOR_TEXT_INACTIVE}):Play()
		end
	end)
	tab.MouseButton1Click:Connect(function()
		setTab(name)
	end)

	tabs[name] = {tab, iconLabel, nameLabel}
	return tab
end

local tabPlayers = createTab("PLAYERS", "PLAYERS", "üë•", 1)
local tabESP = createTab("ESP", "VISUAL", "üëÅÔ∏è", 2)
local tabAimbot = createTab("AIMBOT", "AIMBOT", "üéØ", 3)
