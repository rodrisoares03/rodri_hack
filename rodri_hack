	if espPosition == "Topo" then
		espPosition = "Baixo"
	elseif espPosition == "Baixo" then
		espPosition = "Lado"
	else
		espPosition = "Topo"
	end
	positionBtn.Text = "Posi√ß√£o: " .. espPosition
	for _, p in ipairs(Players:GetPlayers()) do refreshESPForPlayer(p) end
end)

