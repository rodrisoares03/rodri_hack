nightClickBtn.MouseLeave:Connect(function()
	TweenService:Create(nightBtn, TweenInfo.new(0.2), {BackgroundColor3 = COLOR_BG_MAIN}):Play()
	TweenService:Create(nightText, TweenInfo.new(0.2), {TextColor3 = COLOR_NEON_PINK}):Play()
	for _, line in ipairs(dragIcon:GetChildren()) do
